<h1> My Events </h1>

<div>
    <% if @events.blank?%>
        <p> "No events created yet" </p>
    <%else%>
    <ul>
        <% @creator.created_events.each do |event| %>
        <li>
            <strong><%= event.name%> </strong><br>
            <%= event.date%> - <%= event.location%>
        </li>
        <% end %>
    </ul>
    <% end %>
</div>

